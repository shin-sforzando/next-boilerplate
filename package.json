{
  "name": "next-boilerplate",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "reinstall": "node -e 'fs.rmSync(`node_modules`, {recursive:true, force:true})' && npm install",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "check": "biome check --apply .",
    "lint": "biome lint --apply .",
    "format": "biome format --write ."
  },
  "lint-staged": {
    "*.{json,js,jsx,ts,tsx}": [
      "biome check --apply"
    ]
  },
  "volta": {
    "node": "21.6.0",
    "npm": "10.2.4"
  },
  "dependencies": {
    "next": "14.1.0",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "husky": "^9.0.7",
    "lint-staged": "^15.2.1",
    "typescript": "^5"
  }
}
