{
  "name": "next-boilerplate",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "reinstall": "node -e 'fs.rmSync(`node_modules`, {recursive:true, force:true})' && npm install",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "rome check --apply .",
    "format": "rome format --write ."
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "rome check --apply",
      "rome format --write"
    ]
  },
  "volta": {
    "node": "21.6.0",
    "npm": "10.2.4"
  },
  "dependencies": {
    "next": "14.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "20.11.14",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "eslint": "8.56.0",
    "eslint-config-next": "14.1.0",
    "husky": "^9.0.7",
    "lint-staged": "^15.2.1",
    "rome": "^12.1.3",
    "typescript": "5.3.3"
  }
}
